        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Switcherino"
        mc:Ignorable="d"
        Title="MainWindow" Height="190" Width="300">
    <Grid>
        <Button Content="Sign-In Logger" HorizontalAlignment="Left" Margin="10,30,0,0" VerticalAlignment="Top" Width="120" Height="40" Click="SwapSignIn"/>
        <Button Content="Phone/Email Logger" HorizontalAlignment="Left" Margin="150,30,0,0" VerticalAlignment="Top" Width="120" Height="40" Click="SwapPhoneEmail"/>
        <Button Content="Downlaod Data" HorizontalAlignment="Left" VerticalAlignment="Top" Width="120" Height="40" Margin="80,90,0,0" Click="SwapDownloadData"/>

    </Grid>
</Window>
